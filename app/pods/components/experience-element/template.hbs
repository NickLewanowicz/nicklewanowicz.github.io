{{#paper-card as |card|}}
      {{#card.title as |title|}}
        {{#title.text as |text|}}
          {{#text.headline}}{{config.company}}{{/text.headline}}
          {{#text.subhead}}{{config.position}}{{/text.subhead}}
          {{#text.subhead}}{{config.startDate}} to {{config.endDate}}{{/text.subhead}}
        {{/title.text}}
      {{/card.title}}

      {{#card.content class="layout-column layout-align-space-between"}}
        <div class="layout-row">
            <div class=" layout-column">
            {{#each config.highlights as |highlight|}}
                    <div>â€¢ {{highlight}}</div>
            {{/each}}
            </div>
            {{#card.media size="xl"}}
            <div class="card-media">
            
            </div>
            {{/card.media}}
        </div>
        {{#card.actions class="layout-row"}}
          {{#paper-button iconButton=true}}{{paper-icon "favorite"}}{{/paper-button}}
          {{#paper-button iconButton=true}}{{paper-icon "settings"}}{{/paper-button}}
          {{#paper-button iconButton=true}}{{paper-icon "share"}}{{/paper-button}}
        {{/card.actions}}
      {{/card.content}}
    {{/paper-card}}
{{yield}}

